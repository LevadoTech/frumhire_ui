{
  "name": "frumhire_ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm-watch validate\" \"next dev\"",
    "validate": "eslint src",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "eslint": "eslint \"./src/**/*.tsx\" --fix"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^20.16.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.9.0",
    "npm-watch": "0.13.0",
    "postcss": "8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5",
    "typescript-eslint": "^8.7.0"
  },
  "watch": {
    "validate": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,css,png"
    }
  }
}
